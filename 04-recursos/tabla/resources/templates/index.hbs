<div class="vfes-ms-content" id="tabla" style="visibility: hidden;opacity: 0">
    <!-- Sección Tabla -->
    <section class="vf-table">
        <div class="vf-table__container ws10-u--mb8x">
            <table class="vf-table__datalist ws10-u--m0"
