<link href="css/ws10.css" rel="stylesheet" />
<link href="css/styles.css" rel="stylesheet" data-ws10-microcss />

<div class="vfes-ms-content ws10-u-color--bg-light-grey" id="atika" style="visibility: hidden;opacity: 0">
    <!--  Breadcrumbs  -->
    {{> breadcrumbs }}
    <!-- 2/3 DESKTOP-->
    <div class="vf-container-lane">
        <!--  Banner principal -->
        {{> bannerTicket }}
        <!-- PRINCIPAL CONTAINER 2/3 DESKTOP-->
        <div class="ws10-o-wrapper ws10-u--pb4x">
            <div class="ws10-o-layout">
                <div class="ws10-o-layout__item ws10-u--2/3@desktop ws10-u--1/1@tablet">
                    <!-- ADVANTAGES -->
                    {{> advantages }}

                    <!-- VODAFONE TV -->
                    {{> vodafoneTV }}

                    <!-- ATIKA OFFERS -->
                    {{> atikaOffers }}

                    <!-- ATIKA OFFERS -->
                    {{> fiberMobileTv }}

                    <!-- SIN SORPRESAS -->
                    {{> noSurprises }}

                    <!-- STEPS  -->
                    {{> steps }}
                </div>
            </div>
        </div>
    </div>
    <div class="ws10-o-wrapper"  data-js="ticketStopped">

        <!-- OTHER PRODUCTS  -->
        {{> remoteTv }}
    </div>
    {{> popups }}
</div>
{{> sticky }}
<script src="js/ws10.js"></script>
<script src="js/main.js"></script>